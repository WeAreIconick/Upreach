{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#fff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#002329",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "rgba(0,0,0,0.1)",
					"name": "Transparent Overlay",
					"slug": "transparent-overlay"
				},
				{
					"color": "rgb(92,242,142)",
					"name": "Bright Accent",
					"slug": "accent-bright"
				},
				{
					"color": "rgb(80,200,120)",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "rgb(51,130,77)",
					"name": "Dark Accent",
					"slug": "accent-dark"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"rem"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "PublicSans",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/publicsans/PublicSans[wght].ttf"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "PublicSans",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/publicsans/PublicSans-Italic[wght].ttf"
							]
						}
					],
					"fontFamily": "PublicSans",
					"name": "PublicSans",
					"slug": "publicsans"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.38rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.75rem"
					},
					"name": "Extra Large",
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "4rem",
						"min": "3.15rem"
					},
					"name": "Extra Extra Large",
					"size": "3.15rem",
					"slug": "xx-large"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/details": {
				"color": {
					"background": "var(--wp--preset--color--transparent-overlay)"
				},
				"css": "border-radius: 0.75em\n\n& summary {\n  font-size: 1.2em;\n  display: flex;\n  justify-content: space-between;\n}\n\n& summary::after {\n  font-size: 2rem;\n  content: '+';\n  transition: 0.2s;\n  margin-top: -0.5rem;\n}\n\n&[open] > summary::after {\n  transform: rotate(-45deg);\n}\n",
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--60)",
						"right": "var(--wp--preset--spacing--60)",
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.5"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--Text)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--black)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--accent-dark)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--publicsans)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}